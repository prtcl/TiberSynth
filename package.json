{
  "private": true,
  "name": "tibersynth",
  "version": "2.0.0",
  "description": "TiberSynth is an experimental audio synthesizer that runs in your web browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/prtcl/tibersynth.git"
  },
  "author": "Cory O'Brien <cory@prtcl.cc>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prtcl/tibersynth/issues"
  },
  "homepage": "https://github.com/prtcl/tibersynth",
  "scripts": {
    "build": "npm run clean ; npm install ; npm run webpack",
    "clean": "rm -r ./node_modules | true ; rm -r ./www/dist | true",
    "dev": "$(npm bin)/parallelshell 'npm run webpack:watch' 'npm run server:dev'",
    "server": "NODE_ENV=production PORT=8989 node ./app.js",
    "server:dev": "NODE_ENV=development PORT=3000 nodemon ./app.js",
    "start": "npm run dev",
    "webpack": "$(npm bin)/webpack",
    "webpack:watch": "NODE_ENV=development $(npm bin)/webpack --watch --mode=development"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel": "^6.23.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "css-loader": "^2.0.1",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.9",
    "parallelshell": "^3.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "handlebars": "^4.0.12",
    "hashids": "^1.2.2",
    "koa": "^2.6.2",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.5",
    "memoize-one": "^5.0.0",
    "mousetrap": "^1.6.2",
    "react": "^16.6.3",
    "react-compound-slider": "^0.16.3",
    "react-dom": "^16.6.3",
    "react-ionicons": "^3.0.1",
    "react-measure": "^2.2.2",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.2"
  }
}
